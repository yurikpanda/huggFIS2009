    "import numpy as np\n",
    "\n",
    "def f(x):\n",
    "    return x*(x-1)\n",
    "\n",
    "def derivada(x,delta):\n",
    "    return ( (f(x+delta) - f(x)) / delta )\n",
    "\n",
    "for i in range(4,15,2):\n",
    "    #delta = 1 * 10 ** i\n",
    "    print(derivada(1,1 * 10 ** (-i)))\n"
   ]
